{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "wwwroot/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/(.*\\.(js|css|html|ico|png))",
      "dest": "/wwwroot/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/wwwroot/index.html"
    }
  ]
} 